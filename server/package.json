    "cloudinary": "^1.23.0",
    "cookie-parser": "~1.4.5",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.0.0",
    "express": "~4.16.1",
    "express-fileupload": "^1.2.0",
    "express-validator": "^6.6.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.9",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "nodemon": "^1.19.1",
    "uuidv4": "^6.2.4"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn prettier:fix"
    }
